  <%= form_for @user, :url => url_for(:controller => 'users', :action => 'create') do |f| %>
  <%= f.text_field :email, class: 'form-attributes', placeholder: 'email' %>
  <%= f.text_field :name, class: 'form-attributes', placeholder: 'name'  %>
  <%= f.password_field :password, id: "login-password" %>
  <%= f.submit 'Create', class: 'submit-button'%>
  <% end %>